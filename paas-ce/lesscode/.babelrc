{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": "commonjs",
                "targets": {
                    "browsers": ["> 1%", "last 2 versions", "not ie <= 8"],
                    "node": "current"
                },
                "debug": false
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-transform-object-assign",
        "lodash",
        "@babel/plugin-syntax-dynamic-import",
        ["babel-plugin-import-bk-magic-vue", { "baseLibName": "bk-magic-vue" }],
        "transform-vue-jsx"
    ]
}
